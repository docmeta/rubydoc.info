<title>
  <%= h @page_title %>
  <% if options.title && @page_title != options.title %>
    â€“ <%= h options.title %>
  <% end %>
</title>
<script>
  window.yard_library_name = '<%= @library.name %>';
  window.yard_library_version = '<%= @library.version %>';
</script>
<% stylesheets.each do |stylesheet| %>
  <link rel="stylesheet" href="<%= mtime_url(stylesheet) %>" type="text/css" />
<% end %>
<%= erb :script_setup %>
<% javascripts.each do |javascript| %>
  <script type="text/javascript" charset="utf-8" src="<%= mtime_url(javascript) %>"></script>
<% end %>
<div class="nav_wrap">
  <iframe id="nav" src="<%= @nav_url %>?1"></iframe>
  <div id="resizer"></div>
</div>
<div id="main" tabindex="-1">
  <div id="header">
    <%= erb(:breadcrumb) %>
    <%= erb(:search) %>
    <div class="clear"></div>
  </div>
  <div id="content"><%= yieldall %></div>
  <%= erb(:footer) %>
  <% if Rubydoc.config.integrations.clicky %>
    <script src="http://static.getclicky.com/js" type="text/javascript"></script>
    <script type="text/javascript">
      clicky.init(<%= Rubydoc.config.integrations.clicky %>);
    </script>
    <noscript>
      <p><img alt="Clicky" width="1" height="1" src="http://static.getclicky.com/<%= Rubydoc.config.clicky %>ns.gif" /></p>
    </noscript>
  <% end %>
  <% if Rubydoc.config.integrations.google_analytics %>
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
      ga('create', '<%= Rubydoc.config.integrations.google_analytics %>', 'auto');
      ga('send', 'pageview');
    </script>
  <% end %>
</div>
